Algoritmo "Exercicio 5"

// Data atual  : 11/03/2023
Var
   numero, antecessor,sucessor :inteiro
Inicio

   escreval("Digite um numero:  ")
   leia(numero)
   antecessor <- numero-1
   sucessor <- numero +1


   escreval("Numero informado: ", numero)
   escreval("Antecessor :", antecessor)
   escreval("Sucessor:", sucessor)
Fimalgoritmo